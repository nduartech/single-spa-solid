{
  "name": "@nduartech/single-spa-solid",
  "version": "1.1.0",
  "description": "A single spa plugin for SolidJS apps",
  "main": "src/single-spa-solid.js",
  "scripts": {
    "prepublishOnly": "yarn build",
    "lint": "eslint src",
    "format": "prettier --write .",
    "build": "rimraf lib && babel src --out-dir lib --source-maps"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/nduartech/single-spa-solid.git"
  },
  "husky": {
    "hooks": {
      "pre-commit": "pretty-quick --staged && yarn lint"
    }
  },
  "keywords": [
    "single",
    "page",
    "application",
    "spa",
    "multi",
    "one",
    "preact"
  ],
  "files": [
    "lib",
    "src"
  ],
  "author": "Nathan Duarte",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/nduartech/single-spa-solid/issues"
  },
  "homepage": "https://github.com/nduartech/single-spa-solid#readme",
  "devDependencies": {
    "@babel/cli": "^7.10.5",
    "@babel/core": "^7.11.1",
    "@babel/eslint-parser": "^7.11.3",
    "@babel/preset-env": "^7.11.0",
    "eslint": "^7.7.0",
    "eslint-config-important-stuff": "^1.1.0",
    "eslint-plugin-prettier": "^3.1.4",
    "husky": "^4.2.5",
    "prettier": "^2.0.5",
    "pretty-quick": "^2.0.1",
    "rimraf": "^3.0.2"
  }
}
